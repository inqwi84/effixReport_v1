<Application x:Class="EffixReportSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="ApplicationView.xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Skins\Dictionaries\Colors.xaml"/>
                <ResourceDictionary Source="Skins\Dictionaries\Dictionary.xaml"/>
                <ResourceDictionary Source="Skins\Dictionaries\ControlDictionary.xaml"/>
                <ResourceDictionary Source="Skins\Dictionaries\Fonts.xaml"/>
                <ResourceDictionary Source="Skins\Dictionaries\StringDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
